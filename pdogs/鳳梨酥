x1 = int(input(''))
x2 = int(input(''))
y1 = int(input(''))
y2 = int(input(''))

egg = 2 * x1 + 3 * x2
pineapple = 4 * x1 + 3 * x2

if egg % y1 == 0:
    staff_egg = egg // y1
else:
    staff_egg = egg // y1 + 1

if pineapple % y2 == 0:
    staff_pineapple = pineapple // y2
else:
    staff_pineapple = pineapple // y2 + 1

print(str(egg) + ',' + str(pineapple) + ',' + str(staff_egg) + ',' + str(staff_pineapple))