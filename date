import datetime
d1 = datetime.datetime(2002,9,19)
d2 = datetime.datetime(2003,5,9,13,10)
print(d2.date())
print(d2.time())
print(d2.date().weekday())

print("today's date: ", datetime.date.today())

d3 = datetime.datetime(2023,8,3,18,40)
d4 = datetime.datetime(2002,9,19,13,15)
diff = d3 - d4
print(diff)
ts = diff.total_seconds()
print(ts)

d5 = datetime.datetime(2010,3,2,12,15,0)
d6 = datetime.timedelta(days = 145, hours = 10, minutes = 3)
sum = d5 + d6
print(sum)